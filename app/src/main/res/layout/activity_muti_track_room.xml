    <com.qiniu.droid.rtc.demo.ui.UserTrackView
        android:id="@+id/track_window_b"
        android:layout_width="0dp"
        android:layout_height="0dp" />

    <com.qiniu.droid.rtc.demo.ui.UserTrackView
